> spmd.code <- "
+ ### Initial.
+ suppressMessages(library(pbdMPI, quietly = TRUE))
+ init()
+ .comm.size <- comm.size()
+ .comm.rank <- comm.rank()
+ 
+ ### Examples.
+ N <- 5
+ x <- (1:N) + N * .comm.rank
+ y <- allreduce(matrix(x, nrow = 1), op = \"sum\")
+ comm.print(y)
+ 
+ y <- allreduce(x, double(N), op = \"prod\")
+ comm.print(y)
+ 
+ comm.set.seed(1234, diff = TRUE)
+ x <- as.logical(round(runif(N)))
+ y <- allreduce(x, logical(N), op = \"land\")
+ comm.print(y)
+ 
+ ### Finish.
+ finalize()
+ "
