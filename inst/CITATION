citHeader("To cite pbdMPI in a publication use:")

bibentry(
  "Misc",
  title = "{pbdMPI}: R Interface to {MPI}",
  author = c("Wei-Chen Chen", "George Ostrouchov", "Drew Schmidt",
  "Pragneshkumar Patel", "Hao Yu"),
  year = 2022,
  note = "{R} Package, URL https://cran.r-project.org/package=pbdMPI",
  key = "Chen2022pbdMPIpackage"
)

bibentry(
  "Manual",
  title = "A Quick Guide for the {pbdMPI} Package",
  author = c("Wei-Chen Chen", "George Ostrouchov", "Drew Schmidt",
  "Pragneshkumar Patel", "Hao Yu"),
  year = 2012,
  note = "{R} Vignette, URL https://cran.r-project.org/package=pbdMPI",
  key = "Chen2012pbdMPIvignette"
)

